								$html.find('a').append('<span class="mui-badge mui-badge ApplyUav">申请换机中</span>');
							} else if(row['RepairState'] == 2) {
								$html.find('a').append('<span class="mui-badge mui-badge ApplyUav">已同意换机</span>');
