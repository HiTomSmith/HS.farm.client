html,
		body,
		.mui-content {
			height: 100%;
			overflow: hidden;
			list-style-type: none;
		}
		
		.Map {
			width: 100%;
			height: 100%;
		}
		
		.footer {
			position: fixed;
			bottom: 0px;
			width: 100%;
			height: 60px;
			z-index: 998;
			background: white;
		}
		
		.footer>span {
			width: 70%;
			height: 100%;
			display: block;
			text-align: center;
			line-height: 60px;
			float: left;
			color: #15AE3F;
			font-size: 14px;
		}
		
		.footer>button {
			width: 30%;
			background: #15AE3F;
			height: 100%;
			border-radius: 0;
			font-size: 20px;
		}
		
		.Outside {
			height: 300px;
			width: 100%;
			background: white;
			position: fixed;
			bottom: -300px;
			z-index: 999;
		}
		
		.iconfont {
			position: absolute;
			top: 10px;
			right: 20px;
			font-size: 12px;
			opacity: 0.2;
			transform: rotate(90deg);
		}
		.mui-row{
			background: #1F1F1F;
			position: fixed;
			top: 60px;
			z-index: 10000;
			color: white;
			width: 96%;
			text-align: center;
			height: 50px;
			left: 2%;
			border-radius: 4px;
			font-size: 12px;
			line-height: 26px;
			overflow: hidden;
			}
		.mui-row>div{
			border-left:1px solid white;
		}
		.mui-row>div:first-child{
			border:none;
		}