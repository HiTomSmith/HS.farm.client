html,
body,
.mui-content {
	width: 100%;
	height: 100%;
}

.mui-active {
	background: white !important;
}

.brand-items-active {
	padding: 4px 6px;
	background: #1E9FEF;
	color: white;
	border-radius: 4px;
}

.brand>div:nth-child(1):after {
	content: "|";
}

.brand {
	position: relative;
}

.iconfont {
	position: absolute;
	top: 8px;
	right: 0px;
	color: #E4E4E4;
}

.money {
	width: 70% !important;
	border: none !important;
	height: 26px !important;
	margin: 0 !important;
}


/*提交*/

.save {
	border-radius: 4px;
	position: fixed;
	bottom: 0;
	width: calc(100% - 16px);
	margin: 10px 0px;
	z-index: 99;
}


/*脚*/

.footer {
	position: fixed;
	width: 100%;
	height: 50px;
	bottom: 0;
	left: 0;
}

.footer-Discount {
	background: #D6EFFF;
	color: #595959;
	text-align: center;
	height: 28px;
	line-height: 28px;
}

.footer-Pretotal {
	background: #595959;
	height: 100%;
}

.footer-money {
	float: left;
	width: calc(100% - 102px);
	height:50px;
	line-height: 50px;
	padding-left: 16px;
	font-size: 16px;
	color: white;
}

.footer-Purchase {
	float: right;
	width: 102px;
	height: 100%;
	background: #1E9FEF;
	color: white;
	font-size: 18px;
	text-align: center;
	line-height: 50px;
}

.brand-items {
	display: inline-block;
	height: 40px;
	line-height:40px;
	margin: 0 10px;
}

.brand {
	overflow: hidden;
}

.brand-active {
	height: 40px
}

.address {
	height: 72px;
}


/*地址超出省略号*/

.Address-Choice {
	height: 24px;
	margin-top: 6px;
}

.Address-Choice>.iconfont {
	top: 12px;
	right: 14px;
}

.Address-Choice>div:nth-child(1),
.Address-user>div:nth-child(1) {
	float: left;
}

.ChoiceAddress,
.Address-name {
	float: right;
	width: 30%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	text-align: right;
	padding-right: 30px;
}

.Address-user {
	height: 24px;
	margin-top: 6px;
}


/*输入金额*/

.Purchase-items {
	height: 40px;
	line-height: 40px;
	border-top: 1px solid #F5F5F5;
}


/*福利比*/

.circle {
	background: #f5f5f5;
	position: relative;
	text-align: left !important;
	padding-left: 20px;
}

.circle:before {
	width: 4px;
	height: 4px;
	content: ' ';
	display: block;
	background: #1B9FF1;
	position: relative;
	z-index: 100;
	left: -10px;
	top: 22px;
}

.welfares {
	z-index: 99 !important;
}
.Purchase-details{
	margin-top:6px;
}
