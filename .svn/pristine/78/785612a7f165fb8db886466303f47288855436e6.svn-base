html,
body,
.mui-content {
	height: 100%;
	color: #717171;
	padding: 0 !important;
	margin: 0 !important;
}

.colorB {
	color: #15AE3F;
	/*蓝色*/
}

.font_16 {
	font-size: 16px;
}

.mui-content {
	min-height: 100%;
	height: auto;
	background: white !important;
	margin: 0 !important;
	border: 1px solid white;
}

.mui-table-view-cell:after {
	position: absolute;
	right: 0;
	bottom: 0;
	left: 15px;
	height: 1px;
	content: '';
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5);
	background-color: white;
}

li {
	width: 90%;
	height: 70px;
	margin: 0 auto;
	line-height: 60px;
	padding-top: 10px !important;
}

li>button {
	float: right;
	min-width: 60px;
	width: auto;
	margin-right: 10px;
	margin-top: 10px;
	background: white !important;
	color: #717171 !important;
}

.is {
	background: #15AE3F !important;
	color: white !important;
}

#DepartmentId,
#DepartmentId1 {
	border: 1px solid #CCCCCC;
	border-radius: 4px;
	width: 60%;
	float: left;
	height: 40px;
	font-size: 14px;
}

label {
	float: right;
	display: block;
	width: 30%;
	height: 40px;
	line-height: 40px;
	border-radius: 4px;
	margin-right: 10px;
	color: white;
	background: #15AE3F;
	text-align: center;
	font-size: 14px;
}

.AllImg {
	position: relative;
}

.AllImg>input {
	position: absolute;
	z-index: 1;
	opacity: 0;
}

.AllImg>button {
	position: absolute;
	top: 0;
	right: 0;
}

.AllImg>.name {
	position: absolute;
	top: 0;
}

.cbcolor {
	background: #15AE3F !important;
	color: white !important;
}

#isShow {
	display: none;
}