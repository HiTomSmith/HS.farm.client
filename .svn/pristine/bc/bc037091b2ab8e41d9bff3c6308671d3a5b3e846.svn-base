*{
	font-size: 12px;
}
.header{
/*	height: 67px;*/
	background: #15AE3F;
}
ul,li{
	list-style: none;
	margin: 0;
	padding: 0;
}
.header a,.header h1{
	color: #FFFFFF;
	font-size: 18px;
}
.main{
	/*border: 1px solid black;*/
	margin: 0 15px;
	height:auto;
}
.machineModel{
	height: 46px;
	/*border: 1px solid green;*/
}
.machineModel>div{
	float: left;
	height: 44px;
	color: #23241D;
	font-size: 15px;
}
.machineModel>div>div{
	height: 44px;
	text-align: center;
	line-height: 44px;
}
/*.machineModel ul li span{
	margin-left: 4px;
	color: #23241D;
	font-size: 15px;
}*/
