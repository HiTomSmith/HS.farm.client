<script>
	mui.init({});
	var gpsList = [];
	var linarr = [];
	var _self = "";
	var Imei = "";
	//工作量区域数组
	var worknumberList = [];
	//飞行轨迹定义
	var track = XHX_track.creatNew(map2, linarr, "track");
	//	//飞机定义
	//var flyMark = XHX_flymarker.creatNew(map2, [31.01321213123, 121.0], "fly");
	//定时器
	var run = "";
	//清楚所有数组
	var send = [];
	//绘制的对象
	var DrawObj = "";
	var runs = 0;
	//计时器的速度
	var ms = 100;
	//计时器倍数
	var Multiple = 4;
	//飞机定义
	var flyMark = XHX_flymarker.creatNew(map2, [31.01, 121.0], "fly");
	app.ready(function() {

		userInfo = app.accountInfo('info');
		_self = plus.webview.currentWebview();
		Imei = _self.imei;
		
		//获取设备天轨迹
		
		
		
		
		//获取当前分包下所有的作业轨迹
		getWorkNumberNoEstimate(_self.TaskDistributionId);
		//点击事件
		btns();
		//判断如果type==1；则是选择地块创建的任务，可以结束
		if(_self.deletes==0) {
			$("header").append('<a class="mui-pull-right" style="margin-top: 12px;" id="endTask">结束任务</a>');
			//结束任务
			$("#endTask").on('tap', function() {
				var Assessment = plus.webview.getWebviewById('Assessment');
				var SSTaskList = plus.webview.getWebviewById('SSTaskList');
				var str = "";
				$(".list").each(function(i){
					str+=$(".state12").eq(i).text();
				});
				//判断是否有未评估的轨迹
				if(str.indexOf('未评估')!=-1){
					app.toast('还有未评估的轨迹不能结束任务！');
					return false;
				};
				dialog.Dialogbox('任务详情', '是否结束任务', function(cb) {
					if(cb == 1) {
						TaskObj.UserEndTask({
							keyValue: _self.TaskDistributionId,
						}, function(cb) {
							if(cb['type'] == "1") {
								app.toast('任务已结束');
								mui.fire(Assessment, 'pageflowrefresh');
								mui.fire(SSTaskList, 'pageflowrefresh');
								var User = plus.webview.getWebviewById('User');
								mui.fire(User, 'pageflowrefresh');
								mui.back();
							};
							plus.nativeUI.closeWaiting();
						});
					} else if(cb == 0) {
						dialog.CloseLogBox();
					} else {
						app.toast('已取消');
					};
				});
			});
		};
		
		/////任务结束不可评估
		if(_self.deletes==1){
			$("#Measure").attr('disabled','disabled');
		};
	});

	function getWorkNumberNoEstimate(taskDistributionId) {
		var queryJson = {
			ProjectId: taskDistributionId, 
		};
		var option = {
			api: '/XhxManage/XHX_WorkNumber/GetWorkNumberNoEstimate',
			data: {
				queryJson: JSON.stringify(queryJson)
			},
			success: function(data) {
				var result = JSON.parse(data);
				for(var i in result) {
					var row = result[i];
					var latlngs = JSON.parse(result[i].Region);
					var latlng = [];
					for(var j in latlngs) {
						var temp = WGS84_to_GCJ02.creatNew().transform(latlngs[j].Lat, latlngs[j].Lng);
						latlng.push(temp);
					};
					result[i].geo = XHX_region.creatNew(map2, latlng, result[i].WorkNumberId);
					
					var $html = $(
						'<li class="mui-table-view-cell fontText1 list" data-index=' + i + ' IMEI=' + row['IMEI'] + ' value=' + row['UserId'] + ' id=' + row['DataRecordId'] + ' name=' + row['RealName'] + ' phone=' + row['Account'] + '>' +
						'轨迹' + (parseInt(i) + 1) + '&nbsp;&nbsp;&nbsp;<span class="fontText">作业亩数:' + ((row['RegionArea2']!=null)?row['RegionArea2']:row['RegionArea']) + '</span>&nbsp;&nbsp;&nbsp;&nbsp;<span class="fontText">时间:' + row['CreateDate'].substr(0, 10) + '</span>' +
						'<span style="float: right;" class="state12">状态:' + ((row['IsAssessment'] == 0) ? '未评估' : '已评估') + '</span>' +
						'</li>'
					);
					$html.appendTo($(".trajectory>ul"));
				};
				//单条轨迹点击事件
				TrajectoryClick(result); 
				//轨迹条数
				$("#TrajectoryLen").text('（共' + result.length + '条轨迹）');
			},
			error: function() {
				//console.log("接口失败");
			},
		};
		app.post3_2(option);
	}

	//单条轨迹点击事件
	function TrajectoryClick(result) {
		$(".list").each(function(i) {
			$(".list").eq(i).on('tap', function() {
				var index = i;
				var DataRecordId = $(this).attr('id');
				$(".list").each(function(l) {
					$(".list").removeClass('bggray');
				});
				$(".list").eq(index).addClass('bggray');
				$("#TaskPreson").text(result[index]['RealName']).attr('userId', result[index]['UserId']);
				$("#TaskArea").text(((result[index]['RegionArea2']!=null)?result[index]['RegionArea2']:result[index]['RegionArea'])).attr({
					'IMEI': result[index]['IMEI'],
					'WorkNumberId': result[index]['WorkNumberId'],
					'status': result[index]['IsAssessment']
				});
				$("#TaskDate").text(result[index]['SprayLength']);
				$(".pggj>span").text(result[index]['SprayArea']).attr('realArea', result[index]['SprayArea']); //轨迹作业量，俗称喷洒量
				$(".taskmu>span").text(result[index]['RegionArea']).attr('realArea', result[index]['RegionArea']); //区域作业量

				var mker = JSON.parse(result[index]['Region']);
				var position = WGS84_to_GCJ02.creatNew().transform(mker[0]['Lat'], mker[0]['Lng']);
				//				var marker = L.marker(position,{
				//						icon:L.divIcon({
				//							className:'leaflet-icon',
				//		                    iconSize:[5,20],
				//		                    html:'<img src="../../img/XzUav.png" style="width: 20px; height:20px;position: relative;">'
				//	                    })
				//					})
				//					.bindPopup('<p>团队:' + result[index]['TeamName'] + '</p><p>执行人:' + result[index]['RealName'] + '</p><p>作业面积：' + result[index]['RegionArea'] + '亩</p><p>设备编号:' + result[index]['IMEI'] + '</p>')
				//					.addTo(map2)
				//					.openPopup();
				map2.setView(position, 19);
				var DarwData = "";
				getWorkNumberTrack(DataRecordId, DarwData);
			});
		});
	};
	//加载单个作业区域的的作业轨迹
	function getWorkNumberTrack(datarecordId, DarwData) {
		var option = {
			api: '/XhxManage/XHX_GpsRecord/GetWorkNumberTrack',
			data: {
				keyValue: datarecordId
			},
			success: function(data) {
				var result = JSON.parse(data);
				console.log(JSON.stringify(result))
				DrawObj = result;
				runs = 0;
				Multiple = 4;
				$("#Speed").text('1X');
				//切换轨迹停止定时器
				if(run != "") {
					$("#start").show();
					$("#end").hide();
					$(".progress").css({
						"width": 0
					});
					for(var i in send) {
						clearInterval(send[i]);
					};
					run = "";
				};
				track.setLatLngs([]);
				for(var i in result) {

					var row = result[i];
					//坐标转换
					var temp = WGS84_to_GCJ02.creatNew().transform(parseFloat(row.Lat), parseFloat(row.Lng));

					//绘制轨迹
					//flyLine.setPath(linarr);
					track = track.addLatLng(temp);
				};
				$("#startORend").off();
				//轨迹回放 
				$("#startORend").on('tap', function() {
					backBool(result);
					$("#startORend").off();
				});
				//回放
				function backBool(DarwData) {
					track = track.setLatLngs([]);
					$("#start").hide();
					$("#end").show();
					StartDraw(DarwData);
				};

				//开始绘制
				function StartDraw(DarwData) {
					function timer() {
						runs++;
						//播放完成停止定时器
						if(runs >= DrawObj.length) {
							//console.log(runs+'----------------------------'+DrawObj.length);
							for(var i in send) {
								clearInterval(send[i]);
							};
							$(".progress").css({
								"width": "100%"
							});
							$("#start").show();
							$("#end").hide();
							$("#startORend").on('tap', function() {
								$(".progress").css({
									"width": 0
								});
								backBool(DarwData);
								$("#startORend").off();
							});
							runs = 0;
							$("#Speed").off();
						} else {
							var row = DrawObj[runs];
							var temp = WGS84_to_GCJ02.creatNew().transform(parseFloat(row.Lat), parseFloat(row.Lng));
							track = track.addLatLng(temp);
							//飞机定义
							//console.log(parseFloat(row.Lat)+'--------------'+parseFloat(row.Lng));
							//flyMark = XHX_flymarker.creatNew(map2, [parseFloat(row.Lat),parseFloat(row.Lng)],"fly");
							flyMark = flyMark.setLatLng(temp);
							var Prowidth = (runs / DrawObj.length) * 102;
							$(".progress").css({
								"width": Prowidth + '%'
							});
						};
						//暂停定时器
						function StopDraw() {
							for(var i in send) {
								clearInterval(send[i]);
							};
							//console.log(i+'暂停-------------');
						};
						//继续定时器
						function StartDraws() {
							clearInterval(run);
							run = setInterval(timer, ms * Multiple);
							send.push(run);
							//console.log(i+'继续-------------');
						};
						//暂停 、
						$("#end").on('tap', function() {
							$("#start").show();
							$("#end").hide();
							StopDraw();
						});
						//、 继续
						$("#start").on('tap', function() {
							$("#start").hide();
							$("#end").show();
							StartDraws();
						});
					};
					Multiple = 4;
					run = setInterval(timer, ms * Multiple);
					send.push(run);

					//加速播放
					function Multiples() {
						if(Multiple != 0) {
							Multiple--;
							if(Multiple < 1) {
								Multiple = 4;
							};
						};
						console.log(Multiple);
						switch(Multiple) {
							case 4:
								$("#Speed").text('1X');
								break;
							case 3:
								$("#Speed").text('2X');
								break;
							case 2:
								$("#Speed").text('3X');
								break;
							case 1:
								$("#Speed").text('4X');
								break;
						};
						//判断 轨迹是否运行完成
						if(runs >= DrawObj.length) {
							for(var i in send) {
								clearInterval(send[i]);
							};
							Multiple = 4;
						} else {
							for(var i in send) {
								clearInterval(send[i]);
							};
							run = setInterval(timer, ms * Multiple);
							send.push(run);
						};
					};
					$("#Speed").off();
					//修改倍速
					$("#Speed").on('tap', function() {
						//console.log($(".pro").width()+'-------------------'+$(".progress").width());
						if($(".pro").width() >= $(".progress").width()) {
							Multiples();
						};
					});
				};
			},
			error: function() {
				//console.log("接口失败");
			},
		};
		app.post3_2(option);
	}

	///=btns
	function btns() {
		///查看作业轨迹\
		$("#SeeTrajectory").on('tap', function() {
			if($(".trajectory").css('bottom') == "-98px") {
				$(".trajectory").animate({
					'bottom': '52px'
				}).css('overflow-y', 'scroll');
				$(".mui-row").animate({
					'bottom': '242px'
				});
				$(".playController").animate({
					'bottom': '332px'
				});
			} else {
				$(".trajectory").animate({
					'bottom': '-98px'
				}).css('overflow', 'hidden');
				$(".mui-row").animate({
					'bottom': '102px'
				});
				$(".playController").animate({
					'bottom': '192px'
				});
			};
		});

		//开始评估
		$("#Measure").on('tap', function() {
			var zt = "";
			if($(".trajectory").css('bottom')!='52px') {
				app.toast('请选择轨迹，然后进行评估');
 				return false;
			};
			if($("#TaskArea").text() == 0) {
				app.toast('请选择轨迹，然后进行评估');
				return false;
			};
			
			$(".state12").each(function(i){
				zt = $(".state12").eq(i).text();
			});
			if(zt.indexOf('已评估')!=-1) {
				app.toast('当前轨迹已经评估');
				return false;
			}; 
			$(".fontText1").each(function(i) {
				$(".trajectory,.footer").animate({
					'left': '-100%'
				});
				$(".Assessment").animate({
					'left': '0'
				});
				if($(".fontText1").eq(i).hasClass('bggray')) {
					$("#Name").text('飞手姓名:' + $(".fontText1").eq(i).attr('name'));
					$("#Account").text('电话:' + $(".fontText1").eq(i).attr('phone'));
					//获取头像

					var userId = $(".fontText1").eq(i).attr('value');
					UseIdGetName.getUsers(userId, function(cb) {
						//判断头像是否为空
						var HeadIcon = "";
						if(cb['HeadIcon'] != null) {
							HeadIcon = app.req_host + cb['HeadIcon'];
						};
						$(".user>a>img").attr('src', HeadIcon);
					});
				};
				return;
			});
		})

		///关闭评估
		$('#close').on('tap', function() {
			$(".Assessment").animate({
				'left': '100%'
			});
			$(".trajectory,.footer").animate({
				'left': '0'
			});
		});

		///
		$(".fontText1").each(function(i) {
			$(".fontText1").eq(i).on('tap', function() {
				$(".fontText1").removeClass('Selected');
				$(".fontText1").eq(i).addClass('Selected')
			});
		});

		//评估方式 
		$(".modeChildren").each(function(i) {
			$(".modeChildren").eq(i).on('tap', function() {
				$(".modeChildren").css({
					"background": "white",
					"color": "#717171",
					"border": "#c3c3c3"
				}).removeAttr('who');
				$(".modeChildren").eq(i).css({
					"background": " #15AE3F",
					"color": "white",
					"border": " #15AE3F"
				}).attr('who', 'this');
			});
		});

		////作业质量打星
		$(".Task").each(function(i) {
			//&#xe6f4;
			$(".Task").eq(i).on('tap', function() {
				$(".Task").html('&#xe6f5;');
				$(".Task").css("color", "#B8B9C1");
				$(".Task").removeAttr('num');
				for(var j = 0; j < i + 1; j++) {
					$(".Task").eq(j).html('&#xe6f4;');
					$(".Task").eq(j).css("color", "#F29234");
					$(".Task").eq(j).attr('num', 'true');
				};
			});
		});
		
		//规范性
		$(".Normalization").each(function(i) {
			//&#xe6f4;
			$(".Normalization").eq(i).on('tap', function() {
				$(".Normalization").html('&#xe6f5;');
				$(".Normalization").css("color", "#B8B9C1");
				$(".Normalization").removeAttr('num');
				for(var j = 0; j < i + 1; j++) {
					$(".Normalization").eq(j).html('&#xe6f4;');
					$(".Normalization").eq(j).css("color", "#F29234");
					$(".Normalization").eq(j).attr('num', 'true');
				};
			});
		});
		

		///service 服务态度打星
		$(".service").each(function(i) {
			//&#xe6f4;
			$(".service").eq(i).on('tap', function() {
				$(".service").html('&#xe6f5;');
				$(".service").css("color", "#B8B9C1");
				$(".service").removeAttr('num');
				for(var j = 0; j < i + 1; j++) {
					$(".service").eq(j).html('&#xe6f4;');
					$(".service").eq(j).attr('num', 'true');
					$(".service").eq(j).css("color", "#F29234");
				};
			});
		});

		//手绘
		$(".Hand").on('tap', function() {
			drawRegion();

		});

		///XhxManage/XHX_WorkEstimate/SaveForm
		$("#save").on('tap', function() {
			var service = 0;
			var Task = 0;
			var Normalization = 0;
			var type = "";
			var entity1 = {};
			var SaveEntity = [];
			$(".service").each(function(i) {
				if($(".service").eq(i).attr('num') == "true") {
					service++;
				};
			});
			$(".Task").each(function(i) {
				if($(".Task").eq(i).attr('num') == "true") {
					Task++;
				};
			});
			$(".Normalization").each(function(i) {
				if($(".Normalization").eq(i).attr('num') == "true") {
					Normalization++;
				};
			});
			
			$(".modeChildren").each(function(i) {
				if($(".modeChildren").eq(i).attr('who') != undefined) {
					entity1.RegionArea = $(".ss>span").eq(i).attr('realArea');
				};
				type += $(".modeChildren").eq(i).attr('who');
			});
			if(type.indexOf('this') == -1) {
				app.toast('请选择评估方式');
				return false;
			};
			if(Task == 0) {
				app.toast('请为作业质量打分');
				return false;
			};
			if(service == 0) {
				app.toast('请为服务态度打分');
				return false;
			};

			///
			entity1.WorkPersonId = $("#TaskPreson").attr('userid');
			entity1.IMEI = $("#TaskArea").attr('imei');
			entity1.SprayQuality = service * 20;
			entity1.Quality = Task * 20;
			entity1.Normalization = Normalization * 20;
			entity1.EstimateId = userInfo.UserId;
			entity1.SprayTime = new Date().format();
			entity1.WorkNumberId = $("#TaskArea").attr('WorkNumberId');
			entity1.Description = $('.Description').val();
			plus.nativeUI.showWaiting();
			var option = {
				api: '/XhxManage/XHX_WorkEstimate/SaveForm',
				data: {
					entity: entity1
				},
				success: function(data) {
					var result = JSON.parse(data);
					if(result['type'] == 1) {
						app.toast('评估成功');
						$("#TaskArea").attr('status', '1');
						//
						$(".list").each(function(i){
							if($(".list").eq(i).attr('IMEI')==entity1.IMEI){ 
								$(".state12").eq(i).text('状态：已评估');
								$("#TaskArea").attr('status','1');
							};
						});
						$(".Assessment").animate({
							'left': '100%'
						});
						$(".trajectory,.footer").animate({
							'left': '0'
						});
						plus.nativeUI.closeWaiting();
					} else {
						app.toast('评估失败');
						plus.nativeUI.closeWaiting();
					};
				},
				error: function(data) {
					app.toast('评估失败');
				}
			};
			app.post3_1(option);
		});
	};

	function Area(s) {
		var Area = 0;
		if(s.length > 7) {
			Area = ((s / 10000).toFixed(1)) + '万';
		} else {
			Area = s;
		};
		$(".handdraw>span").text(Area).attr('realArea', Area); //手绘工作量
	};
</script>