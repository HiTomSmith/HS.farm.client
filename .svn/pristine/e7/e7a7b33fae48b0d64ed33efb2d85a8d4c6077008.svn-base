		form>ul>li {
			width: 90%;
			height: 70px;
			margin: 0 auto;
			line-height: 60px;
			padding-top: 10px !important;
		}
		
		form>ul>li>button {
			float: right;
			min-width: 60px;
			width: auto;
			margin-right: 10px;
			margin-top: 10px;
			background: white !important;
			color: #717171 !important;
		}
		
		.AllImg {
			position: relative;
		}
		
		.AllImg>input {
			position: absolute;
			z-index: 1;
			opacity: 0;
		}
		
		.AllImg>button {
			position: absolute;
			top: 0;
			right: 0;
		}
		
		.AllImg>.name {
			position: absolute;
			top: 0;
		}
		
		.cbcolor {
			background:  #15AE3F !important;
			color: white !important;
		}
		
		#isShow {
			display: none;
		}
		