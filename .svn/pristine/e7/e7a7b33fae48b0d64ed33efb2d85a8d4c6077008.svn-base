html,body,.mui-content{
			height:100%;
			color:#717171;
		}
		.colorB{
			color:#15AE3F;/*蓝色*/
		}
		.font_16{
			font-size:16px;
		}
		.mui-content{
			width:100%;
		}
		.mui-table-view-cell:after{
			position: absolute;
		    right: 0;
		    bottom: 0;
		    left: 15px;
		    height: 1px;
		    content: '';
		    -webkit-transform: scaleY(.5);
		    transform: scaleY(.5);
		    background-color:white;
		}
		.step{
			width:100%;
			height:100%;
			display: block;
			border-radius: 100%;
			margin:0 auto; 
			line-height: 23px;
		}
		.bgb{
			background:  #15AE3F !important;
			color: white;
		}
		.status{
			min-width: 360px;
			max-width: 360px;
			width:100%;
			height:80px;
			margin:0 auto;
		}
		.MyLease,.Accept,.Receipt{
			width:33%;
			height:100%;
			float: left;
			text-align: center;
		}
		.MyLease>span,.Accept>span,.Receipt>span{
			display: block;
			width: 30px;
			height:30px;
			border-radius: 100%;
			color:#9D958B;
			background: #dedede;
			margin:10px auto;
			line-height: 30px;
			font-size: 12px;
			box-shadow:  0 4px 5px -3px  #CCCCCC inset;
			padding:4px;
		}
		.rowF{
			position: relative;
			z-index:0;
		}
		.rowGay,.rowGay1{
			position: absolute;
			height:10px;
			width:310%;
			background: #dedede;
			top:10px;
			box-shadow:  0 4px 5px -3px #CCCCCC inset;
			z-index: -10000;
		}
		.rowGay{
			right:28px;
			z-index:0;
		}
		.rowGay1{
			left:28px;
		}
		.mengbi{
			position: fixed;
			height:200px;
			width:100%;
			background: rgba(0,0,0,.3);
			display: none;
		}
		.mui-input-row{
			position: relative;
		
		}
		.Arrow{
			position: absolute;
			float: right;
			font-size:12px;
			opacity: 0.2;
			top:18px;
			right:18px;
		}
		
		#getLeaseDetail>li:first-child{
			height:80px;
		}
		#getLeaseDetail>li {
			width: 90%;
			height: 50px;
			margin: 0 auto;
			line-height: 20px;
		}
		
		#getLeaseDetail>li>button {
			float: right;
			min-width: 60px;
			width: auto;
			margin-right: 10px;
			margin-top: 10px;
			background: white ;
			color: #717171 ;
		}
		
		.is {
			background:  #15AE3F !important;
			color: white !important;
		}
		
