		<script src="../../js/app.js"></script>
		<script src="../../js/data-unit.js"></script>
		<script src="../../js/setData.js"></script>
		<script type="text/javascript">
			mui.init()
			var UserInfo = "";
			var waitPay = 0;
			var waitExp = 0;
			var finishOver = 0;
			app.ready(function() {
				UserInfo = app.accountInfo('info');
			});
			$('#feiji').on('tap',function(){
				mui.openWindow('../Lease/FarmLease.html','FarmLease')
			});
			$('#dianchi').on('tap',function(){
				mui.openWindow('../Battery/battery.html','battery')
			});
			$('#wode').on('tap',function(){
				mui.openWindow('../Equipment/EquipmentList.html','EquipmentList')
			});
			$('#shouhou').on('tap',function(){
				mui.toast("暂未开通，敬请期待",{
					type:"div"
				})
			});
			$('#xinjian').on('tap',function(){
				mui.openWindow('../Task/CreatTask.html','CreatTask')
			});
			$('#renwu').on('tap',function(){
				mui.openWindow('../Task/TaskList.html','TaskList')
			});
			$('#guanli').on('tap',function(){
				mui.openWindow('../Team/TeamList.html','TeamList')
			});
			$('#zhanji').on('tap',function(){
				mui.openWindow('../Task/HisTaskList.html','HisTaskList')
			});
			$('#quanbu').on('tap',function(){
				mui.openWindow('../Lease/LeaseList.html','LeaseList')
			});
			$('#quzulin').on('tap',function(){
				mui.openWindow('../Lease/FarmLease.html','FarmLease')
			});
			var sendData = {
				"page": 1,
				"rows": 100,
				"sidx": "CreateDate",
				"sord": "desc",
				"queryJson": JSON.stringify({
					UserInfoId: UserInfo.UserId,
					DeleteMark: undefined,
					LeaseState: 0,
				})
			};
			var option = {
			api: '/PlanManage/LeaseTable/GetPageListJson',
			data: sendData,
			success: function(data) {
				var result = JSON.parse(data);
				console.log(data)
//				plus.nativeUI.closeWaiting();
				var rows = result['rows'];
				for(var i in rows) {
					var row = rows[i];
					console.log(row.LeaseTableId)
					switch(row['LeaseState']) {
						case 0: //未付款
							waitPay++;						
							break;
						case 2: //待收货
							waitExp++;						
							break;
						case 3: //客户已收货		
							finishOver++
							break;
					};
				};
				$("#daifukuan")[0].text(waitPay);
				$("#daishouhuo")[0].text(waitExp);
				$("#yiwancheng")[0].text(finishOver);
			},
			error: function(data) {
				console.log(JSON.stringify("error:" + data))
			}
		};
		app.post3_4(option);
		</script>
	</body>

