html,
body,
.mui-content {
	width: 100%;
	height: 100%;
	background: #F5F5F5;
}

.mui-row {
	background: white;
}

.mui-row>div {
	text-align: center;
}

.black {
	color: #5F5F6B;
}

.gray {
	color: #9D9D9D;
}

.blue {
	color: #1B9FF1;
}

.yellow {
	color: #FB9543;
}


/*飞机型号*/

.UavType {
	margin-top: 44px;
}

.UavType,
.UavTypes {
	height: 80px;
	background: white;
}

.Uav {
	height: 80px;
	width: 100px;
	float: left;
	text-align: center;
	background: #15AE3F;
	color: white;
	position: relative;
}

.Uav:active {
	background: #1E9FEF;
}

.Uav-active {
	background: white;
	color:#15AE3F;
}

.Uav-active:after {
	width: 6px;
	height: 6px;
	content: ' ';
	display: block;
	background: white;
	position: absolute;
	z-index: 100;
	transform: rotate(45deg);
	left: calc(50% - 3px);
	bottom: -4px;
}

.circle {
	position: relative;
	text-align: left !important;
	padding-left: 20px;
}

.circle:before {
	width: 4px;
	height: 4px;
	content: ' ';
	display: block;
	background: #1B9FF1;
	position: relative;
	z-index: 100;
	left: -10px;
	top: 11px;
}


/*飞机图片*/

.Huav {
	margin-top: 10px;
	width: 60px;
	height: 30px;
}


/*套餐选取*/

.GroupType {
	margin-top: 124px;
	min-width: 100%;
	height: 44px;
	background: white;
}

.Groups {
	width: 100px;
	float: left;
	text-align: center;
	height: 44px;
	line-height: 44px;
}

.Groups-active {
	color: #15AE3F;
	border-bottom: 2px solid #15AE3F;
}


/*地址*/

.Address,.Leaseback {
	width: 100%;
	border-top: 1px solid #F5F5F5;
	background: white;
	height: 50px;
}

.Address .goods {
	margin: 14px auto;
}

.goods,
.goodss {
	margin: 20px auto;
	height: 28px;
}

.goodss {
	position: relative;
}

.goodss>div:nth-child(2) {
	width: 40px;
	height: 40px;
	margin: 0 auto;
	border-radius: 100%;
	background: #15AE3F;
	text-align: center;
	color: white;
	line-height: 40px;
}

.row {
	position: absolute;
	line-height: 40px;
}

.row>img {
	width: 80%;
}


/*开始租赁时间*/

.LeaseStartTime,
.StockNum {
	border-top: 1px solid #F5F5F5;
	line-height: 28px;
}

.LeaseStartTime {
	margin-top: 124px;
}


/*选择数量*/

.goods>button {
	padding: 0;
	width: 22px;
	height: 22px;
	text-align: center;
	float: left;
	margin-top: 4px;
}

.icon1>.font_14 {
	float: left;
	width: 40px;
	height: 22px;
	border: 1px solid #CCCCCC;
	display: block;
	margin: 4px 4px;
	border-radius: 4px;
	line-height: 22px;
}


/*脚*/

.footer {
	width: 100%;
	height: 44px;
	position: fixed;
	bottom: 0;
	line-height: 44px;
	border-top: 1px solid #F5F5F5;
}

.total {
	text-align: left !important;
	padding-left: 15px;
}

#shooping,
#pay {
	height: 44px;
	border: none;
	color: white;
	border-radius: 0px;
}

#shooping:active {
	background: #1E9FEF;
}

#pay:active {
	background: #E9F4FF;
}

#shooping {
	background: #E9F4FF;
	color: #15AE3F;
	right: -3px;
}

#pay {
	background: #15AE3F;
}


/*备注*/

.descposition {
	background: #F5F5F5;
}

.descposition>div {
	margin-top: 20px;
}