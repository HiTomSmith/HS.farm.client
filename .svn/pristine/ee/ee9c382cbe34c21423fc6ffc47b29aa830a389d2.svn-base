*{
	font-size: 12px;
}
ul,li{
	list-style: none;
	margin: 0;
	padding: 0;
}
.header{
	height: 100px;
	background: #15AE3F;
}
.header a{
	color: #FFFFFF;
	font-size: 18px;
}
.header h1{
	color: #FFFFFF;
	font-size: 19px;
}
.expressDelivery{
	width: 35%;
	height: 50px;
	float: left;
	color: #9B9B9B;
	line-height: 40px;
	text-align: center;
	background-image: url(../img/peisong.png);
	background-repeat:no-repeat;
	background-size:100% 50px;
	position: absolute;
	/*border: 1px solid red;*/
	padding-right: 10px;
}
.store{
	width: 40%;
	height: 50px;
	float: left;
	line-height: 40px;
	text-align: center;
	color: #9B9B9B;
	background-image: url(../img/ziti.png);
	background-repeat:no-repeat;
	background-size:100% 50px;
	position: absolute;
	left: 95px;
	/*border: 1px solid red;*/
}
.active{
	color: #15AE3F;
}
.mui-content{
	margin-top: 55px;
}
.content{
	margin: 0 10px;
	height: auto;
	/*border: 1px solid green;*/
}
.addAdress{
	display: flex;
	height: 110px;
/*	border: 1px solid green;*/
	justify-content: center;
	align-items: center;
}
.changeBar{
	width: 100%;
	height: 20px;
	/*border: 1px solid red;*/
	display: flex;
	justify-content: center;
	align-items: center;
}
.fillAdress{
	height: 75px;
	/*border: 1px solid green;*/
	background: #FFFFFF;
}
.fillAdress>.fillAdressFirst{
	height: 35px;
	display: flex;
	justify-content: center;
	align-items: center;
	color: #23241D;
	margin-top: 10px;
/*	border: 1px solid orange;*/
	
}
.fillAdress>.fillAdressFirst>.fillAdressExpress{
	text-align: right;
	color: #1CB144;
}
.fillAdress>.fillAdressSecond{
	height: 40px;
	margin-top: 6px;
	color: #9B9B9B;
	/*border: 1px solid orange;*/
}
.addAdresschoose{
	/*margin-top: 40px;*/
	width: 70%;
	height: 40px;
	border: 1px solid green;
	display: flex;
	justify-content: center;
	align-items: center;
	border-radius: 15px;
	color: #1CB144;
}
.goodDetais{
	height: 90px;
	margin-top: 10px;
	/*border: 1px solid green;*/
	background-color: #F4F4F4;
}
.goodDetais p:first-child{
	color: #23241D;
	font-size: 15px;
}
.goodDetais p:nth-child(2){
	color: #9B9B9B;
	font-size: 12px;
}
.goodDetais>div:first-child{
	display:flex;
	height: 90px;
	justify-content: center;
	align-items: center;
	
}
.logoCenter{
	display:flex;
	justify-content: center;
	align-items: center;
}
.goodDetais>div:nth-child(2){
	height: 90px;
	padding-left: 15px;
	padding-top: 20px;
}
.remarks{
	height: 50px;
	/*border: 1px solid green;*/
	margin-top: 10px;
	border-bottom: 1px solid #F4F4F4;
}
.remarks span{
	color: #23241D;
	font-size: 13px;
}
.remarks>div:first-child{
	height: 50px;
	display: flex;
	justify-content: center;
	align-items: center;
	/*border: 1px solid red;*/
}
.remarks>div:nth-child(2){
	height: 50px;
	padding-top: 6px;
}
.remarks textarea{
	color: #9B9B9B;
	font-size: 12px;
	border: none;
}
.feeList{
	color: #23241D;
	font-size: 12px;
}
.feeList>.cleanUnderline{
	border-bottom: 1px solid #F4F4F4;
	padding-bottom: 10px;
}
.moneyRight{
	text-align: right;
}
.feeList .tai{
	color: #9B9B9B;
}
.feeList .taiMiddle{
	text-align: center;
}
.feeList>div{
	margin-top: 15px;
}
.feeList .coupon,.feeList .total{
/*	border: 1px solid red;*/
	margin-top: 0px;
}
.coupon span{
	color: red;
	margin-left: 5px;
}
.coupon>div:nth-child(2){
	text-align: right;
}
.total{
	font-size: 13px;
	border-bottom: 1px solid #F4F4F4;
}
.total>div:nth-child(2){
	text-align: right;
	color: #15AE3F;
	font-size: 15px;
}
.total span{
	margin-left: 5px;
}
.agreement{
	margin-top: 15px;
	font-size: 11px;
	border-bottom: 1px solid #F4F4F4;
	height: 40px;
}
.agreement span{
	color: #9B9B9B;
}
.agreement a{
	color: #15AE3F;
}
.footer{
	width: 100%;
	position: fixed;
	bottom: 10px;
	margin-left: 8%;
	margin-right:8%;
}
.footer button{
	width: 40%;
	height: 40px;
	background-color: #15AE3F;
	color: #FFFFFF;
	font-size: 14px;
	text-align: center;
	border-radius: 20px;
	border: none;
}
.footer button:first-child{
	margin-right: 4px;
}


