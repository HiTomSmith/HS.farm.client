<script>
	mui.init();
	var UserInfo = "";
	var renewList = []; //续租数据
	app.ready(function() {
		UserInfo = app.accountInfo('info');
		//初始化租赁单状态
		PlanPayLease(undefined, 0);
		NoData.bgImg('mui-content', '../../img/BGIMG.png');
		//getRenewList();
	});
	mui(".mui-scroll-wrapper").scroll({
		bounce: true, //滚动条是否有弹力默认是true
		indicators: false, //是否显示滚动条,默认是true
	});

	var status1 = [
		'<span class="font_12 UavStatus2">等待支付</span>',
		'<span class="font_12 UavStatus3">已付款</span>',
		'<span class="font_12 UavStatus4">商家已发货</span>',
		'<span class="font_12 UavStatus5">客户已收货</span>',
		'<span class="font_12 UavStatus6">申请退租</span>',
		'<span class="font_12 UavStatus7">客户已发货</span>',
		'<span class="font_12 UavStatus8">商家已收货</span>',
		'<span class="font_12 UavStatus9">维修</span>',
		'<span class="font_12 UavStatus10">完成</span>',
		'<span class="font_12 UavStatus11">已经失效</span>',
	];
	status1[18] = '<span class="font_12 UavStatus">已激活</span>';
	status1[19] = '<span class="orderStatus mui-pull-right">正常使用</span>';

	//先按续租状态排序，再按续租结束日期排序
	var sortTwoCate = function(a, b) {

		 
		if(a["RenewState"] === b["RenewState"]) {    
			if(a["RenewEndDate"] > b["RenewEndDate"]) {      
				return 1;    
			} else if(a["RenewEndDate"] < b["RenewEndDate"]) {      
				return -1;    
			} else {      
				return 0;    
			}  
		} else {    
			if(a["RenewState"] < b["RenewState"]) {      
				return 1;    
			} else {      
				return -1;    
			}  
		}

	}

	//获取打算支付的租赁单
	function PlanPayLease(type, deleteMarker, s, e) {
		var sendData = {
			"page": 1,
			"rows": 100,
			"sidx": "CreateDate",
			"sord": "desc",
			"queryJson": JSON.stringify({
				UserInfoId: UserInfo.UserId,
				DeleteMark: deleteMarker,
				LeaseState: type,
			})
		};
		if(s && e) {
			sendData = {
				"page": 1,
				"rows": 100,
				"sidx": "CreateDate",
				"sord": "desc",
				"queryJson": JSON.stringify({
					UserInfoId: UserInfo.UserId,
					DeleteMark: deleteMarker,
					LeaseState: type,
					StartTime: s,
					EndTime: e,
				})
			}
		};

		var Lease = plus.webview.getWebviewById('Lease');

		mui.fire(Lease, 'pageflowerfresh');
		var option = {
			api: '/PlanManage/LeaseTable/GetPageListJson',
			data: sendData,
			success: function(data) {

				var sendData = {
					queryJson: JSON.stringify({
						UserInfoId: UserInfo.UserId
					})
				};
				console.log("续租参数：" + sendData)
				getLeaseInterface.getRenewList(sendData, function(cb) {
					renewList = cb;

					console.log("续租结果：" + JSON.stringify(renewList))

					var result = JSON.parse(data);
					console.log(data)
					//两个参数，代付款，待收货
					var waitPay = 0;
					var waitExp = 0;
					plus.nativeUI.closeWaiting();
					if(result['rows'].length >= 1) {
						NoData.ClearImg('mui-content');
					};

					var rows = result['rows'];
					for(var i in rows) {
						var row = rows[i];
						console.log(row.LeaseTableId)
						var $html = "";
						console.log("状态：" + row['LeaseState'])
						switch(row['LeaseState']) {
							case 19:

								var html_renew = ''

								////排序
								var renewList = renewList;
								renewList.sort(sortTwoCate);

								//								renewList.sort(function(a, b) {
								//									return b.RenewState - a.RenewState;
								//								});

								for(var n in renewList) { //续租数据
									var renewRow = renewList[n];
									if(renewRow.LeaseTableId == row['LeaseTableId']) {

										console.log("重复数：" + JSON.stringify(renewRow))
										html_renew = html_renew + '<div class="mui-row deletList" data-index=' + i + ' style="color: #777777;font-size: 13px;">' +
											'<div class="mui-col-sm-4 mui-col-xs-5">结束时间:<span>' + (renewRow.RenewEndDate).substr(0, 10) + '</span></div>' +
											'<div class="mui-col-sm-2 mui-col-xs-2" style="color: #222222;">￥<span>' + renewRow.RenewNumber * renewRow.Money + '</span>	</div>' +
											'<div class="mui-col-sm-3 mui-col-xs-3" >' + ((renewRow.RenewState == 0) ? '未付款' : '已完成') + '</div>' +
											'<div class="mui-col-sm-2 mui-col-xs-2" style="text-align: right;">' + ((renewRow.RenewState == 0) ? '<a class="deleteRenew" style="color: #EF2020;" id=' + renewRow['LeaseTableRenewId'] + '   data-index="' + i + '">删除</a>' : '') + ' </div>' +
											'</div>'

									}
								}

								$html = $(
									'<div class="perListContent" data-index=' + i + '>' +
									'<div class="orderTitle">' + status1[row['LeaseState']] + '<span class="orderTime">' + row['LeaseBeginDate'].substr(0, 10) + '</span></div>' +

									'<div class="orderDetails">' +

									'<div class="orderImg">' +
									'<img src="../../img/1s.png" width="100%" height="100%"/>' +
									'</div>' +

									'<div class="orderdes">' +
									'<p class="liRightText">0押金租大疆<span class="mgMode" style="margin-left: 3px;">' + row.GoodsName + '</span></p>' +
									'<p class="liRightCenter">型号：<span class="mode">' + row.SN + '</span></p>' +
									'<p class="liRightRight"><span class="mui-pull-right">x1</span>￥<span class="mode">' + row.PreTotal + '</span></p>' +
									'</div>' +

									'</div>' +
									'<div class="btnList">' +
									'<div class="orderBtn">' +
									'<button type="button" class="btnRight nowRenew">立即续租</button>' +
									'</div>' +
									'</div>' +

									'</div>' +

									'<div>' + html_renew + '</div>'
								);

								$html.appendTo($("#addList"));

								CalculationDays(row['LeaseBeginDate'], row['LeaseEndDate'], i);
								//立即续租
								$html.on('tap', function() {
									var index = $(this).attr('data-index');
									mui.openWindow({
										url: './RenewalDetals.html',
										id: 'RenewalDetals',
										extras: {
											data: rows[index]
										}
									})
								});

								//删除未审核的续租订单
								$html.find('.deleteRenew').on('tap', function(event) {
									event.stopPropagation();
									var id = $(this).attr('id');
									var index = $(this).attr('data-index');
									var sendData = {
										keyValue: id
									};
									console.log("新id：" + JSON.stringify(sendData))
									dialog.Dialogbox('提示', '确认删除当前续租订单？', function(cbs) {
										if(cbs == 1) {
											getLeaseInterface.delRenewOrder(sendData, function(cb) {
												console.log("删除：" + JSON.stringify(cb))
												if(cb['type'] == 1) {
													app.toast('删除成功!');
													$('.deletList').eq(index).remove();
													var _page = plus.webview.getWebviewById('RenewalList');
													_page.reload(true);
												} else {
													app.toast('删除失败');
												};
												dialog.CloseLogBox();
											});
										} else if(cbs == 0) {
											dialog.CloseLogBox();
										} else {
											app.toast('已取消');
										};
									});
								});
								break;
						};
					};

				});

			},
			error: function(data) {
				console.log(JSON.stringify("error:" + data))
			}
		};
		app.post3_4(option);
	};
	//获取续租列表
	function getRenewList() {
		var sendData = {
			queryJson: JSON.stringify({
				LeaseTableId: ''
			})
		};
		console.log("续租参数：" + sendData)
		getLeaseInterface.getRenewList(sendData, function(cb) {
			renewList = cb;

		});
	}

	///获取物流单主键
	function getExpId(leaseId) {
		var option = {
			api: '/PlanManage/Express/GetPageListJson',
			data: {
				"page": 1,
				"rows": 1,
				"sidx": "CreateDate",
				"sord": "desc",
				"queryJson": JSON.stringify({
					LeaseTableId: leaseId,
				})
			},
			success: function(data) {
				var result = JSON.parse(data);
				var rows = result['rows'];
				for(var i in rows) {
					var row = rows[i];
					var Compy = row['ExpressCompany'];
					var Num = row['ExpressRealId'];
					mui.openWindow({
						url: "https://www.kuaidi100.com/chaxun?com=" + Compy + "&nu=" + Num + "",
						id: 'ExpWindow',
						styles: {
							top: '44px'
						}
					});
				};
			},
			error: function(data) {}
		};
		app.post3_4(option)
	};
	//计算租赁单剩余支付时间
	function CalculationTime(stime, i) {
		var createTime = Date.parse(stime); //创建时间
		var enTime = createTime + 7200000; //结束时间
		var newTime = new Date();
		var newTimeMs = Date.parse(newTime); //当前时间戳
		var ChaTimes = enTime - newTimeMs; //剩余毫秒
		//剩余分钟
		var SurplusTime = ChaTimes / (1000 * 60);
		var SurplusTime1 = SurplusTime.toFixed(0);
		if(SurplusTime1 <= 0) {
			SurplusTime1 = 0;
			return status1[9] + '<span class="mui-badge Surplus" id="state2" data="失效">' + SurplusTime1 + '分钟</span>';
		} else {
			return status1[0] + '<span class="mui-badge Surplus" id="state2">剩余:' + SurplusTime1 + '分钟</span>';
		};
	};

	///计算剩余天数
	function CalculationDays(start, end, i) {
		var Days = ((Date.parse(end) - Date.parse(new Date())) / 86400000).toFixed(0);
		if(Days <= 0) {
			Days = 0;
		};
		$(".leaseList").eq(i).append('<span class="mui-badge">剩余' + Days + '天数</span>');
	};
	window.addEventListener('pageflowerfresh', function() {
		$("#getNoPayList").html('');
		PlanPayLease(undefined, 0);
		$(".mui-row>div>.tittle").each(function() {
			$(".mui-row>div>.tittle").removeClass('activeT');
		});
		$(".mui-row>div>.tittle").eq(0).addClass('activeT');
	});

	window.addEventListener('pageflowerfresh1', function() {
		location.reload();
	});
	window.addEventListener('fresh', function() {
		location.reload();
	});
</script>